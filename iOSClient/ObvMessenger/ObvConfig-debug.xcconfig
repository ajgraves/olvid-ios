OBV_BUNDLE_DISPLAY_NAME_FOR_SHARE_EXTENSION = $(inherited) Debug
OBV_PRODUCT_BUNDLE_IDENTIFIER_FOR_SHARE_EXTENSION = io.olvid.messenger-debug.extension-share

